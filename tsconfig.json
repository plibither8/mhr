{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ES2020",
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "ES2021",
    "baseUrl": ".",
    "rootDir": "./",
    "paths": {
      "src/*": ["./src/*"]
    },
    "types": ["@cloudflare/workers-types"]
  },
  "lib": ["ESNext"],
  "include": ["src/**/*.ts"]
}
